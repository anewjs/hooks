{
  "name": "@anew/hooks",
  "version": "0.1.0",
  "description": "AnewJS React Hooks",
  "repository": "git@github.com:anewjs/hooks.git",
  "bugs": "https://github.com/anewjs/hooks/issues",
  "keywords": [
    "react",
    "hooks",
    "anew",
    "store"
  ],
  "author": "Abdelrahmam Salem <abubakir1997@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "lib/anewjs-hooks.js",
  "unpkg": "dist/anewjs-hooks.js",
  "module": "es/anewjs-hooks.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "types"
  ],
  "scripts": {
    "clean": "rimraf lib dist es coverage types",
    "format": "prettier --write \"{src,test}/**/*.{js,ts}\" \"**/*.md\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.{js,ts}\" \"**/*.md\"",
    "lint": "eslint --ext js,ts src test",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build": "rollup -c",
    "pretest": "yarn run build",
    "prepublishOnly": "yarn run clean && yarn run check-types && yarn run format:check && yarn run lint && yarn test"
  },
  "peerDependencies": {
    "@anew/store": "^2.7.0",
    "react": "^16.9.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "@types/node": "^14.0.19",
    "@types/react": "^16.9.41"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-external-helpers": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-proposal-private-methods": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "glob": "^7.1.6",
    "jest": "^25.4.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "rxjs": "^6.5.5",
    "typescript": "^3.8.3",
    "typings-tester": "^0.3.2"
  },
  "npmName": "@anew/hooks",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "jest": {
    "testRegex": "(/test/.*\\.spec\\.[tj]s)$",
    "coverageProvider": "v8"
  },
  "sideEffects": false
}
